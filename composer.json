{
    "name": "firexsantos/esign-bsre-codeigniter",
    "description": "Package E-Sign BSrE untuk memudahkan penggunaan E-Sign BSrE (Balai Sertifikasi Elektronik) API dari BSSN (Badan Sandi dan Siber Negara)",
    "keywords": [
        "firexsantos",
        "esign-bsre-codeigniter"
    ],
    "homepage": "https://github.com/firexsantos/esign-bsre-codeigniter",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Firman Santosa",
            "email": "firman.snts@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "FirmanSantosa\\ESignBsreCodeIgniter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FirmanSantosa\\ESignBsreCodeIgniter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
